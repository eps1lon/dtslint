{
  "name": "tslint-rule-no-redundant-jsdoc",
  "version": "0.3.0",
  "description": "TSLint rule to forbid JSDoc.",
  "files": [
    "./*Rule.js",
    "./*Rule.js.map",
    "./*Rule.d.ts"
  ],
  "main": "tslint-config.json",
  "contributors": [
    "Andy Hanson <andy-ms@microsoft.com> (https://github.com/andy-ms)",
    "Dan Vanderkam <danvdk@gmail.com> (https://github.com/danvk)"
  ],
  "repository": "eps1lon/dtslint",
  "devDependencies": {
    "dtslint-rule-test-runner": "^0.3.0",
    "typescript": "~2.9.0"
  },
  "peerDependencies": {
    "tslint": "^5.10.0",
    "typescript": "^2.9.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.10.0"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "build:watch": "tsc -w -p ./tsconfig.json",
    "lint": "tslint -p ./tsconfig.json --format stylish",
    "test": "dtslint-rule-test-runner"
  }
